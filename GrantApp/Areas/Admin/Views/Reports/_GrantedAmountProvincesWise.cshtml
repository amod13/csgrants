@model GrantApp.Models.ReportModel
@{
    int usertype = GrantApp.Areas.Admin.FunctionClass.GetCurrentLoginUserType();

    int i = 1;
    decimal GrandTotalAmount = 0;
    decimal GrandVDCMUNAmount = 0;
    decimal GrandTotalRequestAmt = 0;
    decimal FirstInstallment = 0, SecondInstallment = 0, ThirdInstallment = 0;
}
<div class="table-responsive">
    <table class="table table-hover" id="DetailsReportsID">
        <thead>
            <tr>
                <th>क्रम</th>
                <th>आ.व.</th>
                <th>स्थानिय तहको नाम</th>
                <th>मुलभुत क्षेत्र</th>
                <th>आयोजनाको कोड</th>
                <th>आयोजनाको नाम</th>
                <th>आयोजनाको अवधि</th>
                <th>कूल लागत</th>
                <th>सम्बन्धित तहले व्यहोर्ने</th>               
                <th>प्रथम</th>
                <th>दोस्रो</th>
                <th>तेस्रो</th>
                
                <th></th>
            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model.ComplementryReportViewModelList)
            {

                decimal TotalBudgetAmt = item.TotalBudget;
                decimal TotalAmountByVDCMUN = item.AmountProvinceVdc;
                decimal RequestAmt = item.TotalBudget - item.AmountProvinceVdc;
                decimal ValFirstInstalment = item.FirstInstallment;
                decimal ValSecondInstallment = item.SecondInstallment;
                decimal ValThridInstallment = item.ThirdInstallment;
                GrandTotalAmount = GrandTotalAmount + item.TotalBudget;
                GrandVDCMUNAmount = GrandVDCMUNAmount + TotalAmountByVDCMUN;
                GrandTotalRequestAmt = GrandTotalRequestAmt + RequestAmt;

                FirstInstallment = FirstInstallment + ValFirstInstalment;
                SecondInstallment = SecondInstallment + ValSecondInstallment;
                ThirdInstallment = ThirdInstallment + ValThridInstallment;

                <tr>
                    <td scope="row">@i</td>
                    <td>@item.PhaseTitle</td>
                    <td>@item.OfficeName</td>
                    <td>@item.SectionName</td>
                    <td>@item.SubProgramId</td>
                    <td>@item.SubProgramTitle</td>
                    <td>@item.TimeDurationYear</td>
                    <td>@item.TotalBudget</td>
                    <td>@item.AmountProvinceVdc</td>                    
                    <td>@item.FirstInstallment</td>
                    <td>@item.SecondInstallment</td>  
                    <td>@item.ThirdInstallment</td>                    


                    <td style="width:85px;">
                        @Html.ActionLink(" ", "ViewDeatilsListForSubmited", new { @id = item.SubProgramId, @id1 = item.GrantTypeId }, new { @class = "edits fa fa-eye" }) ||
                        @Html.ActionLink(" रकम", "ProgramwiseAmount", new { @id = item.SubProgramId, @id1 = item.GrantTypeId }, new { @class = "" })
                    </td>

                </tr>

                ++i;
            }
            <tr style="font-weight:bold;">
                <td colspan="9">जम्मा रकम</td>
                <td>@FirstInstallment</td>
                <td>@SecondInstallment</td>
                <td>@ThirdInstallment</td>
            </tr>


        </tbody>
    </table>

</div>

